<%= render 'errors', object: @card %>
<h2>Создание новой карточки</h2>
<%= simple_form_for(@card, html: { class: 'form-horizontal' }) do |f| %>
    <%= render "form", f: f %>
    <%= f.submit "Создать", class: "btn btn-sm btn-success" %> <%= link_to "Назад", :back, class: "btn btn-sm btn-primary" %>
<% end %>
